  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "prod": "prettier --write . && next build && next start",
    "pre": "prettier --write . && next lint && tsc --noEmit",
    "pre:build": "prettier --write . && next lint && tsc --noEmit && next build",
    "build": "next build",
    "start": "next start",
    "check": "next lint && tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "update": "npm-check-updates -u && npm i",
    "update:nextjs": "npm i next@latest react@latest react-dom@latest eslint-config-next@latest"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.13",
