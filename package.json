    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lenis": "^1.3.1",
    "lucide-react": "^0.507.0",
    "motion": "^12.10.0",
    "next": "15.3.2",
    "next-themes": "^0.4.6",
    "npm-check-updates": "^18.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react": "^19.1.0",
